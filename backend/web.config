<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="PythonHandler" path="*" verb="*" modules="httpPlatformHandler" resourceType="Unspecified" />
    </handlers>
    <httpPlatform processPath="python.exe" arguments="-m gunicorn --bind=0.0.0.0:8000 --workers=4 --timeout=600 app:app" startupTimeLimit="120" stdoutLogEnabled="true" stdoutLogFile="D:\home\LogFiles\python.log">
      <environmentVariables>
        <environmentVariable name="PYTHONPATH" value="D:\home\site\wwwroot\backend" />
      </environmentVariables>
    </httpPlatform>
    <staticContent>
      <mimeType fileExtension=".json" mimeType="application/json" />
    </staticContent>
  </system.webServer>
</configuration>

